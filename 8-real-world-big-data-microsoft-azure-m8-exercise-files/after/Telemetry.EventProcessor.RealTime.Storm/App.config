<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="EventsDb.ConnectionString" value="Data Source={YOUR-SQL-CONNECTION-STRING}" />
    <add key="EventsHBase.ClusterUrl" value="https://{YOUR-CLUSTER-NAME}.azurehdinsight.net" />
    <add key="EventsHBase.Username" value="admin" />
    <add key="EventsHBase.Password" value="{YOUR-ADMIN-PASSWORD}" />
  </appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>
</configuration>
