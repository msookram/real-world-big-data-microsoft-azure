<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="StorageEmulatorPath" value="C:\Program Files (x86)\Microsoft SDKs\Azure\Storage Emulator\WAStorageEmulator.exe" />
    <add key="LoggingEnabled" value="true" />
    <add key="EnvironmentName" value="dev" />
    <add key="ConfigCacheTimespan" value="00:00:01" />
    <!-- Deep Storage-->
    <add key="DeepStorage.OutputConnectionString" value="UseDevelopmentStorage=true;" />
    <add key="DeepStorage.OutputContainerName" value="test-events" />
    <!-- Event Stores -->
    <add key="EventStores.MemoryEventStore.MaxBufferSize" value="8388608" />    <!-- 8388608 = 8MB -->
    <add key="EventStores.DiskEventStore.MaxBufferSize" value="524288" />    <!-- 524288 = 0.5MB -->
    <add key="EventStores.BlobStorageEventStore.MaxBufferSize" value="4194304" />    <!-- 4194304 = 4MB -->
    <add key="EventStores.OverdueFlushPeriod" value="00:00:02" />
  </appSettings>
  <system.runtime.caching>
    <memoryCache>
      <namedCaches>
        <add name="Telemetry.Config.Cache" cacheMemoryLimitMegabytes="5" />
      </namedCaches>
    </memoryCache>
  </system.runtime.caching>
  <runtime>
